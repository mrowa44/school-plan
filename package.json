{
  "name": "school-plan",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://mrowa44.github.io/school-plan",
  "dependencies": {
    "@netguru/react-scripts": "^2.0.2",
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "lodash.compact": "^3.0.1",
    "moment": "2.24.0",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "fix": "yarn run stylelint --fix && yarn run eslint --fix",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "eslint": "eslint \"src/**/*.js\"",
    "predeploy-prod": "rm -rf build/ && yarn build",
    "deploy-prod": "gh-pages -d build",
    "predeploy-staging": "rm -rf build/ && yarn build",
    "deploy-staging": "gh-pages -d build --remote staging --repo https://github.com/mrowa44/super-private-school-plan-version-omg"
